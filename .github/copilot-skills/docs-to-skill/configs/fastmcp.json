{
  "name": "fastmcp",
  "description": "FastMCP framework for building Model Context Protocol servers",
  "base_url": "https://gofastmcp.com/",
  "max_pages": 100,
  "rate_limit": 0.5,
  "file_patterns": [
    "**/*.py",
    "**/mcp/**",
    "**/*server*.py"
  ],
  "selectors": {
    "main_content": "main, article, div[role=\"main\"], .content",
    "title": "h1, title",
    "code_blocks": "pre code, code"
  },
  "url_patterns": {
    "include": [
      "^https://gofastmcp\\.com/.*"
    ],
    "exclude": [
      "/blog/.*",
      "#.*"
    ]
  },
  "categories": {
    "getting_started": [
      "getting-started",
      "installation",
      "setup",
      "welcome"
    ],
    "guides": [
      "guide",
      "how-to",
      "tutorial"
    ],
    "api": [
      "api",
      "reference",
      "tools"
    ],
    "examples": [
      "example",
      "demo"
    ]
  },
  "checkpoint": {
    "enabled": true,
    "interval": 25
  }
}