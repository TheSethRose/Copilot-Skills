{
  "name": "radix-ui",
  "description": "Unstyled, accessible components for building React applications",
  "base_url": "https://www.radix-ui.com/primitives/",
  "start_urls": [
    "https://www.radix-ui.com/primitives/docs"
  ],
  "max_pages": 250,
  "rate_limit": 0.5,
  "file_patterns": [
    "**/*.tsx",
    "**/*.jsx",
    "**/*.ts",
    "**/package.json"
  ],
  "selectors": {
    "main_content": "main, article, [role='main']",
    "title": "h1, title",
    "code_blocks": "pre code, code[class*='language']",
    "headings": "h1, h2, h3, h4, h5, h6"
  },
  "url_patterns": {
    "include": ["^https://www\\.radix-ui\\.com/primitives/docs/.*"],
    "exclude": ["/blog/.*", "#.*", "/case-studies/.*"]
  },
  "categories": {
    "installation": ["installation", "install", "setup", "getting-started"],
    "components": ["accordion", "alert-dialog", "alert", "aspect-ratio", "avatar", "badge", "button", "checkbox", "collapsible", "combobox", "context-menu", "dialog", "dropdown-menu", "form", "hover-card", "input", "label", "menubar", "navigation-menu", "popover", "primitive", "progress", "radio-group", "scroll-area", "select", "separator", "slider", "slot", "switch", "table", "tabs", "toast", "toggle", "toggle-group", "tooltip"],
    "utilities": ["utility", "utils", "helper", "hook"],
    "theming": ["theme", "theming", "dark-mode", "colors", "styling"],
    "api": ["api", "reference"]
  },
  "checkpoint": {
    "enabled": true,
    "interval": 30
  }
}
