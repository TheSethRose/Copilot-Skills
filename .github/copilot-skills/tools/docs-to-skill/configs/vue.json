{
  "name": "vue",
  "description": "Progressive JavaScript framework for building user interfaces",
  "base_url": "https://vuejs.org/",
  "start_urls": ["https://vuejs.org/guide/introduction.html"],
  "max_pages": 150,
  "rate_limit": 1.0,
  "file_patterns": [
    "**/*.vue",
    "**/*.js",
    "**/*.ts"
  ],
  "selectors": {
    "title": "h1, title",
    "main_content": "main, article, .content",
    "code_blocks": "pre code"
  },
  "url_patterns": {
    "include": ["^https://vuejs\\.org/.*"],
    "exclude": ["#.*", "/examples/.*"]
  },
  "categories": {
    "guide": ["guide", "essentials", "components"],
    "api": ["api"],
    "tutorial": ["tutorial"]
  },
  "checkpoint": {
    "enabled": true,
    "interval": 30
  }
}
