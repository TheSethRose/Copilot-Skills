{
  "name": "swift",
  "description": "Swift programming language - official Apple documentation",
  "base_url": "https://developer.apple.com/documentation/swift/",
  "max_pages": 200,
  "rate_limit": 0.5,
  "file_patterns": ["**/*.swift", "**/*.playground"],
  "selectors": {
    "main_content": "main, article, .content, [role='main']",
    "title": "h1, h2",
    "code_blocks": "pre code, pre"
  },
  "url_patterns": {
    "include": ["^https://developer\\.apple\\.com/documentation/swift/.*"],
    "exclude": ["/videos/.*", "#.*"]
  },
  "categories": {
    "basics": ["basics", "fundamentals"],
    "language": ["language-features", "syntax"],
    "stdlib": ["standard-library", "collections"],
    "concurrency": ["concurrency"],
    "reference": ["reference"]
  },
  "checkpoint": {"enabled": true, "interval": 40}
}
