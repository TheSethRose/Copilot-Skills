{
  "name": "jest",
  "description": "Delightful JavaScript testing framework",
  "base_url": "https://jestjs.io/docs/",
  "max_pages": 100,
  "rate_limit": 1.0,
  "file_patterns": [
    "**/*.test.js",
    "**/*.spec.js",
    "**/jest.config.js"
  ],
  "selectors": {
    "main_content": "main, article, .docContainer",
    "title": "h1",
    "code_blocks": "pre code"
  },
  "url_patterns": {
    "include": ["^https://jestjs\\.io/docs/.*"],
    "exclude": ["/blog/", "#.*"]
  },
  "categories": {
    "getting_started": ["getting-started", "setup"],
    "using_matchers": ["using-matchers"],
    "testing_async": ["testing-async-code"],
    "setup_teardown": ["setup-teardown"],
    "mock_functions": ["mock-functions"],
    "testing_framework": ["snapshot-testing"],
    "configuration": ["configuration"]
  },
  "checkpoint": {
    "enabled": true,
    "interval": 30
  }
}
